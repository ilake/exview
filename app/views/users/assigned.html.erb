<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user_path(user) %> |
      <%= link_to "Share Photo with #{user.name}", new_photo_path(:receiver_id => user) %>
      <%= link_to "Send Message to #{user.name}", new_user_message_path(current_user, :to => user.name) %>
    </li>
  <% end %>
</ul>
